select\040curdate();
select\040timediff(birth,\040curdate())\040from\040pet;
select\040timediff(curdate(),\040birth)\040from\040pet\040where\040death\040is\040null;
select\040curdate(),\040birth\040from\040pet\040where\040death\040is\040null;
select\040curdate(),\040birth,\040timediff(curdate(),\040birth)\040from\040pet\040where\040death\040is\040null;
select\040curdate(),\040birth,\040timediff(curdate(),\040birth)\040as\040ABC\040from\040pet\040where\040death\040is\040null;
select\040curdate(),\040birth,\040(YEAR(CURRENT_DATE)-YEAR(`birth`))-(RIGHT(CURRENT_DATE,5)<RIGHT(`birth`,5)
\040\040)\040as\040ABC\040from\040pet\040where\040death\040is\040null;
select\040curdate(),\040birth,\040(YEAR(CURRENT_DATE)-YEAR(`birth`))-(RIGHT(CURRENT_DATE,5)<RIGHT(`birth`,5)\040\040\040)\040as\040ABC\040from\040pet\040where\040death\040is\040null;
select\040curdate(),\040birth,\040(YEAR(CURRENT_DATE)-YEAR(`birth`))-(DATE_FORMAT(CURRENT_DATE,\040'%m%d')\040<\040DATE_FORMAT(birth,\040'%m%d'))\040as\040ABC\040from\040pet\040where\040death\040is\040null;
select\040count(timediff(curdate(),\040birth))\040from\040pet\040where\040death\040is\040null;
select\040count(owner\040=\040'Harold')\040from\040pet;
select\040count(name)\040from\040pet\040where\040owner\040=\040'Harold';
select\040owner,\040count(owner)\040as\040number_of_pets\040from\040pet\040group\040by\040owner;
select\040owner,\040count(owner)\040as\040number_of_pets\040from\040pet\040group\040by\040owner\040order\040by\040owner;
