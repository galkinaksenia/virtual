select\040id,\040name,\040owner,\040species\040from\040pet;
select\040id,\040name,\040owner,\040species\040from\040pet\040where\040owner\040=\040'Harold';
select\040*\040from\040pet\040where\040owner\040like\040'Harold';
select\040*\040from\040pet\040order\040by\040birth;
select\040*\040from\040pet\040order\040by\040birth\040desc;
